<!-- /////////////////////// HEADER /////////////////////////////////////// -->
<div class="title">
	Welcome to Tick.Get
</div>
<div class="ticketintro">
Buy, trade, & sell your concert tickets
</div>
<!-- ////////////////////// END HEADER /////////////////////////////////// -->
<!-- //////////////////////////NEW TICKET PATH ////////////////////////////-->

<br>
<div class="ticket">
<% @tickets.each do |t| %>
<%= t.concert_name %><br>
<%= t.date_time %><br>
<%= t.venue %><br>
<%= t.price %><br>
<%= t.quantity %><br>
<%= t.trade_value %><br>
<br>
<!-- ////////////////////// END NEW TICKET PATH //////////////////////////  -->
<!-- //////////////////// PATH BUTTONS //////////////////////////////////// -->
<div class="buttonOne">
<p id="search"><button type="button"><%= link_to 'More Information', t %></button></p>
</div>

<div class="buttonTwo">
<p id="search"><button type="button"><%= link_to 'Create a New Ticket', new_ticket_path %></button></p>
</div>
<!-- //////////////////// END PATH BUTTONS //////////////////////////////// -->
<% end %>
</div>


