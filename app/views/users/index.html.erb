
 


<br>
<div class="ticket">
<% @tickets.each do |t| %>
<%= t.concert_name %><br>
<%= t.date_time %><br>
<%= t.venue %><br>
<%= t.price %><br>
<%= t.quantity %><br>
<%= t.trade_value %><br>
<%= link_to 'More Information', t%><br>
<% end %>
</div>

<h1>Listing users</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Password</th>
      <th>Name</th>
      <th>Photo</th>
      <th>Location</th>
   	  <th>Remember Token</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <body>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.password %></td>
        <td><%= user.name %></td>
        <td><%= user.photo %></td>
        <td><%= user.location %></td>
        <td><%= user.remember_token %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %>
        <td><%= button_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Create a New Ticket', new_ticket_path %>